<div class="d-flex justify-content-center pt-3">
  <h1>Dashboard</h1>
</div>
<div class="d-flex justify-content-start">
  <h2>Your listed items</h2>
</div>
<div class="row">
<% @items.each do |item| %>
    <div class="col-lg-4 col-md-6 mb-4">
      <div class="item-card">
        <div class="card-img" style="background-image: url('<%= cl_image_path item.image.key %>');">
        </div>
        <%= link_to item_path(item), class: "text-decoration-none" do %>
        <div class="text-decoration-underline"><h5><%=  item.name %></h5>
        </div>
      <div><p>Category: <%= item.category_name%>
    </div>
  <% end %>
<% end %>
</div>
